{
  "compilerOptions": {
    "baseUrl": ".",
    "strict": false,
    "strictPropertyInitialization": false,
    "noImplicitAny": false,
    "noImplicitThis": false,
    "noImplicitReturns": false,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "importHelpers": true,
    "lib": [
      "DOM",
      "ES2020"
    ],
    "types": [
      "node",
      "obsidian-typings"
    ],
    "isolatedModules": true,
    "module": "ESNext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "noEmit": true,
    "target": "ES2018",
    "skipLibCheck": true
  },
  "include": [
    "./src/**/*.ts",
    "./scripts/**/*.ts"
  ],
  "exclude": [
    "node_modules",
    "templates/**/*.ts",
    "templates/**/*.json"
  ]
}