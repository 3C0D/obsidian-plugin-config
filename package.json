{
  "name": "obsidian-plugin-config",
  "version": "1.0.1",
  "description": "Centralized configuration for Obsidian plugin development",
  "type": "module",
  "main": "src/index.ts",
  "keywords": [
    "obsidian",
    "obsidian-plugin",
    "typescript",
    "shared-config",
    "centralized-config",
    "plugin-utilities",
    "modular-architecture",
    "development-tools"
  ],
  "scripts": {
    "start": "yarn install && yarn run update-exports",
    "update-exports": "node scripts/update-exports.js",
    "acp": "tsx scripts/acp.ts",
    "update-version": "tsx scripts/update-version-config.ts",
    "v": "tsx scripts/update-version-config.ts",
    "migrate-config": "tsx scripts/migrate-config.ts",
    "migrate": "tsx scripts/migrate-config.ts",
    "m": "tsx scripts/migrate-config.ts",
    "help": "tsx scripts/help.ts",
    "h": "tsx scripts/help.ts"
  },
  "exports": {
    ".": "./src/index.ts",
    "./scripts/*": "./scripts/*",
    "./modals": "./src/modals/index.ts",
    "./tools": "./src/tools/index.ts",
    "./utils": "./src/utils/index.ts"
  },
  "devDependencies": {
    "@types/node": "^22.15.26",
    "@types/semver": "^7.7.0",
    "builtin-modules": "3.3.0",
    "dedent": "^1.6.0",
    "dotenv": "^16.4.5",
    "esbuild": "latest",
    "fs-extra": "^11.2.0",
    "obsidian": "*",
    "semver": "^7.7.2",
    "tsx": "^4.19.4",
    "typescript": "^5.8.2"
  },
  "dependencies": {
    "@types/lodash": "^4.17.17",
    "lodash": "^4.17.21"
  },
  "engines": {
    "npm": "please-use-yarn",
    "yarn": ">=1.22.0"
  }
}